{
  "name": "ver-final",
  "version": "1.0.0",
  "description": "To Do list",
  "main": "index.js",
  "dependencies": {
    "del": "^6.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglify": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.4",
    "browser-sync": "^2.26.13",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "gulp": "^4.0.2",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "postcss": "^8.1.13",
    "stylelint": "^13.8.0",
    "stylelint-config-htmlacademy": "^0.1.4"
  },
  "scripts": {
    "lint": "eslint ./js",
    "lint:fix": "eslint ./js --fix --color",
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/less/**/*.less\" --syntax less",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp styles",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "editorconfig-cli": [
    "*.json",
    "./*.html",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/less/**/*.less"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "js/*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC"
}
